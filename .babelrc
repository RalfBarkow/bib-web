{
  "env": {
    "browser": {
      "presets": [
        "es2015", "stage-2", "react"
      ],
      "plugins": [
        "transform-async-to-generator",
        "transform-class-properties",
        "transform-object-rest-spread",
        ["transform-runtime", {
          "helpers": false,
          "polyfill": false,
          "regenerator": true
        }]
      ]
    }
  },
  "presets": ["stage-2", "react"],
  "plugins": [
    "transform-object-rest-spread",
    "transform-class-properties"
  ]
}