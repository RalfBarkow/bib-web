{
  "name": "zotero-bib-web",
  "license": "AGPL-3.0",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "repository": "",
  "main": "",
  "config": {
    "port": "8001",
    "translation_server": "http://localhost:1969"
  },
  "browserslist": "last 2 versions",
  "scripts": {
    "serve": "node scripts/server.js -p $npm_package_config_port -t $npm_package_config_translation_server",
    "prebuild": "npm run clean -s && mkdir -p build/static/",
    "clean": "rimraf build",
    "clean:data": "rimraf data",
    "build": "npm run build:styles-json && run-p 'build:js -s' 'build:scss -s' 'build:html -s' 'build:static -s' && npm run build:postprocess",
    "browserify": "NODE_ENV=browser browserify -s ZoteroBibComponent --extension=.jsx src/js/main.js -t [babelify] -o build/static/bib-component.js",
    "minify": "minify build/static/bib-component.js --mangle --simplify --builtIns --mergeVars --removeConsole --removeDebugger -o build/static/bib-component.js",
    "build:js": "run-s browserify minify",
    "build:scss": "for f in src/scss/*.scss; do node-sass $f --output-style compressed --output build/static/; done;",
    "build:html": "node scripts/build-html.js",
    "build:styles-json": "node scripts/build-styles-json.js",
    "build:static": "mkdir -p build/static && rsync -vazL src/static/* build/static/",
    "build:postprocess": "postcss build/static/bib.css --use autoprefixer --no-map -r",
    "devel": "run-p 'devel:js -s' 'devel:scss -s' 'devel:html -s' 'devel:static -s'",
    "devel:js": "watchify -s ZoteroBibComponent --extension=.jsx src/js/main.js -d -v -t [babelify] -p mapstraction -o build/static/bib-component.js",
    "devel:scss": "nodemon -q -w src/scss --ext 'scss' --exec 'npm run devbuild:scss'",
    "devel:html": "nodemon -q -w src/html -w config --ext '.' --exec 'npm run build:html'",
    "devel:static": "nodemon -q -w src/static --ext '.' --exec 'npm run build:static'",
    "devbuild:scss": "for f in src/scss/*.scss; do node-sass $f --recursive --source-map true --source-map-contents true --output build/static; done;",
    "start": "mkdir -p build/static && run-p \"build:styles-json\" \"serve -s {@}\" \"devel -s {@}\"",
    "stylelint": "stylelint src/scss/**/*.scss || true",
    "eslint": "eslint \"src/js/**/*.+(js|jsx)\" || true",
    "lint": "run-p stylelint eslint",
    "watch:stylelint": "nodemon -q -w src/scss --ext 'scss' --exec 'npm run stylelint'",
    "watch:eslint": "nodemon -q -w src/js --ext 'js,jsx' --exec 'npm run eslint'",
    "watch:lint": "run-p 'watch:stylelint -s' 'watch:eslint -s'",
    "preshrinkwrap": "rimraf ./node_modules/reactstrap/node_modules/react-popper && npm-run json -I -f ./package-lock.json -e 'delete this.dependencies.reactstrap.requires[\"react-popper\"]' -e 'this.dependencies.reactstrap.dependencies[\"react-popper\"] = { version: \"0.8.2\" }'"
  },
  "dependencies": {
    "array-equal": "^1.0.0",
    "babel-regenerator-runtime": "^6.5.0",
    "classnames": "^2.2.5",
    "copy-to-clipboard": "^3.0.8",
    "deep-equal": "^1.0.1",
    "es6-promise": "^4.1.1",
    "file-saver": "^1.3.3",
    "isomorphic-fetch": "^2.2.1",
    "load-script": "^1.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.1.1",
    "react-key-handler": "^1.0.1",
    "react-modal": "^3.3.2",
    "react-popover": "^0.5.4",
    "react-popper": "0.8.2",
    "react-router-dom": "^4.2.2",
    "react-select": "^1.2.1",
    "react-transition-group": "<2.0.0",
    "reactstrap": "^5.0.0-beta.2",
    "smooth-scroll": "^12.1.5",
    "zotero-api-client": "^0.9.4",
    "zotero-api-client-cache": "^0.2.0",
    "zotero-bib": "github:zotero/bib#v0.7.0-release",
    "zotero-web-library": "^0.9.45-alpha"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.5",
    "babel-eslint": "^8.2.1",
    "babel-minify": "^0.2.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^14.5.0",
    "config": "^1.28.1",
    "eslint": "^4.15.0",
    "eslint-plugin-react": "^7.5.1",
    "fs-extra": "^5.0.0",
    "handlebars": "^4.0.11",
    "http-proxy": "^1.16.2",
    "http-server": "^0.10.0",
    "json": "^9.0.6",
    "mapstraction": "^1.0.1",
    "minimist": "^1.2.0",
    "node-sass": "^4.7.2",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.2",
    "postcss-cli": "^4.1.1",
    "rimraf": "^2.6.2",
    "serve-static": "^1.13.1",
    "stylelint": "^8.4.0",
    "stylelint-config-recommended-scss": "^3.0.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-scss": "^2.2.0",
    "uglifyify": "^4.0.5",
    "watchify": "^3.9.0",
    "webworkify": "^1.5.0"
  }
}
